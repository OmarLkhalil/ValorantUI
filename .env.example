# Firebase Configuration
# Copy this file to .env and fill in your actual values
# DO NOT COMMIT .env to git!

# Firebase App ID (from Firebase Console → Project Settings → Your App)
# Format: 1:123456789:android:abc123def456
FIREBASE_APP_ID=your_firebase_app_id_here

# Firebase CLI Token (from: firebase login:ci)
FIREBASE_TOKEN=your_firebase_token_here

# Optional: Firebase Project ID
FIREBASE_PROJECT_ID=your_project_id

# Optional: Keystore for signing (if you have one)
# KEYSTORE_PATH=/path/to/your/keystore.jks
# KEYSTORE_PASSWORD=your_keystore_password
# KEY_ALIAS=your_key_alias
# KEY_PASSWORD=your_key_password

