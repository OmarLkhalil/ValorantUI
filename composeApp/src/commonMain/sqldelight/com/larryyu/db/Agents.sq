CREATE TABLE Agents (
    uuid TEXT NOT NULL PRIMARY KEY,
    displayName TEXT,
    fullPortrait TEXT,
    roleDisplayName TEXT,
    fullPortraitV2 TEXT
);

selectAllAgents:
SELECT * FROM Agents;

insertAgent:
INSERT OR REPLACE INTO Agents(uuid, displayName, fullPortrait, roleDisplayName, fullPortraitV2)
VALUES (?, ?, ?, ?, ?);

clearAgents:
DELETE FROM Agents;

